{
  "name": "cypress-mochawesome-to-mysql",
  "version": "1.0.0",
  "description": "Lab to transform the report data generated by cypress into a mySQL database. The database can later be read by grafana",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "report:setup": "mkdir report",
    "report:clean": "rmdir mochawesome-report /s /q && rmdir report /s /q",
    "db:create": "node ./dist/database.config",
    "db:extract": "node ./dist/extract.report",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:merge": "mochawesome-merge ./mochawesome-report/*.json > ./report/output.json",
    "setup": "run-s build db:create",
    "test": "run-s cy:run",
    "report": "run-s report:setup cy:merge",
    "extract": "run-s build db:extract report:clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quatoo/cypress-mochawesome-to-mysql.git"
  },
  "keywords": [
    "cypress",
    "test",
    "typescript",
    "mochawesome",
    "mysql",
    "transformation",
    "observability",
    "devops"
  ],
  "author": "Filipe Borges",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/quatoo/cypress-mochawesome-to-mysql/issues"
  },
  "homepage": "https://github.com/quatoo/cypress-mochawesome-to-mysql#readme",
  "devDependencies": {
    "@types/mochawesome": "^6.2.1",
    "cypress": "^9.6.1",
    "mochawesome": "^7.1.3",
    "mysql2": "^2.3.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "mochawesome-merge": "^4.2.1"
  }
}
