{
  "name": "cypress-mochawesome-to-mysql",
  "version": "1.0.0",
  "description": "Lab to transform the report data generated by cypress into a mySQL database. The database can later be read by grafana",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "db:create": "node ./dist/database.config",
    "db:load": "aqui vai o run do dist/extract.report",
    "cy:open": "cypress open",
    "cy:run": "cypress run --reporter mochawesome",
    "test": "cypress run",
    "setup": "run-s build db:create"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quatoo/cypress-mochawesome-to-mysql.git"
  },
  "keywords": [
    "cypress",
    "test",
    "typescript",
    "mochawesome",
    "mysql",
    "transformation",
    "observability",
    "devops"
  ],
  "author": "Filipe Borges",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/quatoo/cypress-mochawesome-to-mysql/issues"
  },
  "homepage": "https://github.com/quatoo/cypress-mochawesome-to-mysql#readme",
  "devDependencies": {
    "cypress": "^9.6.1",
    "jsonpath": "^1.1.1",
    "mochawesome": "^7.1.3",
    "mysql2": "^2.3.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "dotenv": "^16.0.1"
  }
}
